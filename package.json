{
  "name": "@newdash/npm-update-all",
  "description": "update all dependencies for project",
  "version": "0.1.10",
  "bin": {
    "npm-update-all": "lib/index.js",
    "nua": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/npm-update-all"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prebuild": "rimraf lib",
    "test": "jest --passWithNoTests",
    "build": "tsc",
    "lint": "eslint src --fix",
    "release": "npx standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.21.0",
    "cli-progress": "^3.9.1",
    "colors": "^1.4.0",
    "commander": "^8.3.0",
    "debug": "^4.3.3",
    "inquirer": "^8.2.0",
    "node-fetch": "^3.1.1",
    "registry-url": "^5.1.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@types/cli-progress": "^3.9.2",
    "@types/colors": "^1.2.1",
    "@types/debug": "^4.1.7",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.3",
    "@types/node-fetch": "^2.5.12",
    "@types/semver": "^7.3.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "commitlint": "^13.2.1",
    "eslint": "^8.5.0",
    "husky": "^4.3.8",
    "jest": "^27.4.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
